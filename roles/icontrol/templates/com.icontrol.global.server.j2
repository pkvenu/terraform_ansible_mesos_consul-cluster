###############################################################################
# This is the global data-center (or server-cluster-specific) file
# which is used to configure the server back end.
###############################################################################


# This is the current services file
servicesFile= "{{services_File}}"

# how often to monitor status URLs in the services.xml file
servicesMonitorSecs= "{{services_Monitor_Secs}}"

# how often to monitor services.xml file
servicesFileMonitorSecs= "{{services_File_Monitor_Secs}}"

# Internal services.  They are required for 4-tier service.
# gatewayRegistryServerURL = http://server.icontrol.com/rc
