#############################################################################
# IP address masks for using sms.war
#
# SMSOUTIPADDRESSMASK
# - IP addresses that should be able to send sms messages
# - on distributed env app servers, use 0.0.0.0/0 (all clients)
# - on distributed env gapp servers, use 255.255.255.255/32 (no clients)
# - on other env configs, use IP address mask for all local servers eg. 10.150.101.0/24
#
# SMSINIPADDRESSMASK
# - IP addresses that should be able to call back into server (from sms aggregator)
# - for any IP use 0.0.0.0/0 (all clients)
# - for openmarket only, use IP mask for openmarket servers
#############################################################################
smsInIPAddressMask= "{{sms_In_IP_Address_Mask}}"
smsOutIPAddressMask= "{{sms_Out_IP_Address_Mask}}"


